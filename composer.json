{
    "name": "Teleservice/MsSQLLinux",
    "description": "MsSQLLinux plugin for CakePHP, Used to handling MsSQL in linux",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "MsSQLLinux\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MsSQLLinux\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
